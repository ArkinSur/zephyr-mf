{
  "name": "HostApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "lint": "eslint .",
    "pods": "(cd ios && bundle install && bundle exec pod install)",
    "pods:update": "(cd ios && bundle exec pod update)",
    "start": "react-native start",
    "start:ios": "react-native start --platform ios",
    "start:android": "react-native start --platform android",
    "test": "jest"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "0.79.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@callstack/repack": "^5.1.3",
    "@eslint/js": "^9.32.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.5",
    "@react-native/eslint-config": "0.79.5",
    "@react-native/metro-config": "0.79.5",
    "@react-native/typescript-config": "0.79.5",
    "@rspack/core": "^1.3.4",
    "@swc/helpers": "^0.5.17",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4",
    "typescript-eslint": "^8.39.0"
  },
  "engines": {
    "node": ">=18"
  }
}
