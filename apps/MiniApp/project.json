{
  "name": "MiniApp",
  "targets": {
    "ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn workspace MiniApp start:ios",
        "cwd": "apps/MiniApp"
      }
    },
    "standalone": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn workspace MiniApp start:standalone",
        "cwd": "apps/MiniApp"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint . --ext .js,.jsx,.ts,.tsx",
        "cwd": "apps/MiniApp"
      }
    }
  }
}
